Frame.Size = UDim2.new(0, 369, 0, 217)

UICorner.Parent = Frame

made.Name = "made"
made.Parent = Frame
made.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
made.BackgroundTransparency = 1.000
made.Position = UDim2.new(0.624563158, 0, 0.719957471, 0)
made.Size = UDim2.new(0, 139, 0, 60)
made.Font = Enum.Font.TitilliumWeb
made.Text = "Made By Make_hack000"
