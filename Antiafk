close.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
close.BackgroundTransparency = 1.000
close.Position = UDim2.new(0.948502064, 0, 0.0276497696, 0)
close.Size = UDim2.new(0, 11, 0, 19)
close.Font = Enum.Font.FredokaOne
close.Text = "X"
close.TextColor3 = Color3.fromRGB(213, 0, 0)
close.TextScaled = true
close.TextSize = 14.000
close.TextStrokeColor3 = Color3.fromRGB(197, 0, 0)
close.TextWrapped = true
local bb=game:service'VirtualUser'
game:service'Players'.LocalPlayer.Idled:connect(function()
	bb:CaptureController()bb:ClickButton2(Vector2.new())
	on.Text="안티킥이 성공적으로 작동 되었습니다 ㅊㅋ"wait(3)on.Text="ON"end)
local text = name

local add = 10
wait()
local k = 1
while k <= 255 do
	text.TextColor3 = Color3.new(k/255,0/255,0/255)
	k = k + add
	wait()
end
while true do
	k = 1
	while k <= 255 do
		text.TextColor3 = Color3.new(255/255,k/255,0/255)
		k = k + add
		wait()
	end
	k = 1
	while k <= 255 do
		text.TextColor3 = Color3.new(255/255 - k/255,255/255,0/255)
		k = k + add
		wait()
	end
	k = 1
	while k <= 255 do
		text.TextColor3 = Color3.new(0/255,255/255,k/255)
		k = k + add
		wait()
	end
	k = 1
	while k <= 255 do
		text.TextColor3 = Color3.new(0/255,255/255 - k/255,255/255)
		k = k + add
		wait()
	end
	k = 1
	while k <= 255 do
		text.TextColor3 = Color3.new(k/255,0/255,255/255)
		k = k + add
		wait()
	end
	k = 1
	while k <= 255 do
		text.TextColor3 = Color3.new(255/255,0/255,255/255 - k/255)
		k = k + add
		wait()
	end
	while k <= 255 do
		text.TextColor3 = Color3.new(255/255 - k/255,0/255,0/255)
		k = k + add
		wait()
	end
end
